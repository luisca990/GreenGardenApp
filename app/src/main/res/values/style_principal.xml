        <item name="android:layout_height">110dp</item>
        <item name="android:layout_margin">10sp</item>
    </style>

    <style name="IconBack">
        <item name="android:scaleType">fitCenter</item>
        <item name="android:background">@drawable/icon_back</item>
        <item name="android:layout_width">110dp</item>
        <item name="android:layout_height">110dp</item>
        <item name="android:layout_margin">10sp</item>
    </style>

</resources>